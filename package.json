    "mocha": "^5.1.1",
    "openzeppelin-solidity": "^1.12.0",
    "solhint": "^1.2.1",
    "solidity-coverage": "^0.5.7",
    "truffle": "4.1.12",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.1",
    "web3": "^1.0.0-beta.34"
  },
  "devDependencies": {
    "@celo/typechain": "git+ssh://git@github.com:celo-org/TypeChain.git#ebcd5b4",
